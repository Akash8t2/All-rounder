{
  "name": "all-rounder-bot",
  "description": "auto create a script for sms panel",
  "repository": "https://github.com/Akash8t2/All-rounder",
  "logo": "https://telegram.org/img/t_logo.png",
  "keywords": ["telegram", "bot", "login", "username", "password"],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "MASTER_BOT_TOKEN": {
      "description": "Telegram Bot Token from @BotFather",
      "required": true
    },
    "MONGO_URI": {
      "description": "Mongo Db Url For Bot Data Base",
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "standard-1x"
    }
  },
  "scripts": {
    "postdeploy": "echo 'All Rounder Bot deployed successfully!'"
  }
}
